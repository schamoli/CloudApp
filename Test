<script src="https://app.talygen.com/Scripts/jquery.validate.min.js" type="text/javascript"></script>
<script src="https://app.talygen.com/Scripts/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>
<script src="https://app.talygen.com/Scripts/jquery.password-strength.min.js" type="text/javascript"></script>
<script type="text/javascript">

    $(document).ready(function () {
        $('#username').focus();
        // Start Region for Dialog Box
        $('#popDetails').dialog({
            autoOpen: false,
            width: 498, 
            resizable: false,
            modal: true,
            title: false,
            header: false,
            open: function (event, ui) {
                    $(this).load("/Account/popSubscription");
            }
        });
        // End Region for Dialog Box
         $('#ResetPassword').dialog({
            autoOpen: false,
            width: 600,
            resizable: false,
            title: Globalize.localize('resetpassword'),
            modal: true,
            open:function(event,ui){                             
                $(this).load("/Account/OpenPopUpToResetPassword", { userId:  '', emailId: ''},showLoader());            
            },
            close:function(){
                $("#ResetPassword").html('');
                window.location = "account/LogOff";
            },
            buttons: {
                "Reset": function () {
                        var form = $("#PopResetPassword");
                        var val = $(form).validate().form();
                        var request = form.serialize();
                        if(val)
                        {          
                            showLoader();                   
                            $.post(
                                '/Account/ResetPasswordWithPolicy', 
                                request, 
                                function (data) {
                                    hideLoader();
                                    if (data != "null") {
                                        $("#ResetPassword").html(data);
                                        var hdnSt=$(data).find("input[id='hdnStatusMsg']");
                                        var hideDiaolog = true;
                                        if(hdnSt.length>0) {
                                            if($(hdnSt).val()=="fail")
                                                hideDiaolog = false;
                                        }
                                        if(hideDiaolog)
                                           window.location='/MyProfile/Index';                                                   
                                        }
                                });
                        }
                    }
                }
                
            });



        if ($('#hdnNotifyMsg').val() == "3") {
            $('#popDetails').dialog('open');
        }
        else if ('' == "6") {
            $('#ResetPassword').dialog('open');
        }

    });

</script>

            </div>
            <!--Login box ends here-->
        </div>
        <!--Body content ends here-->
    </div>
    <!--main wrapper starts here-->   
